graph Simper  [[main]]
{
    // simper
    input filter.mode;
    input filter.frequency;
    input filter.q;
    input filter.gain;

    input stream float<2> in;
    output stream float<2> out;

    node filter             = std::filters (float<2>)::simper::Processor;

    connection
    {
        in -> filter.in;
        filter.out -> out;
    }
}

